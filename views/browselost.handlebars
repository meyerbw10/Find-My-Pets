<h1>hello</h1>


{{#each losts as |lost|}}
<section class="card">
  <a href="/models/Found.js">
  <div class="card border-dark mb-3" style="max-width: 18rem;">
  <div class="card-header">Found!</div>
  <div class="card-body text-dark">
    <h5 class="card-title">{{lost.name}}</h5>
    <h5 class="card-title">{{lost.breed}}</h5>
    <h5 class="card-title">{{lost.sex}}</h5>
    <h5 class="card-title">{{lost.color}}</h5>
    <h5 class="card-title">{{lost.dateLost}}</h5>
    <h5 class="card-title">{{lost.city}}</h5>
    <h5 class="card-title">{{lost.weight}}</h5>
  </div>
</div>

{{/each}}